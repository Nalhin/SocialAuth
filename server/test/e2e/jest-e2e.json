{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "./../../",
  "testEnvironment": "node",
  "testRegex": ".e2e-test.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage-e2e",
  "collectCoverageFrom": [
    "src/**/*.ts",
    "!src/**/*.{spec,test}.ts"
  ]
}
